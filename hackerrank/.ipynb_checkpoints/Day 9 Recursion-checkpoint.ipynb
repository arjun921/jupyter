    "def factorial(n):\n",
    "    print('N= {} before function call'.format(n))\n",
    "    if n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        print('Intermediate n = {}'.format(n))\n",
    "        return n*factorial(n-1)\n",
